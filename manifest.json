{
    "name": "com.ubuntu.developer.bobo1993324.udropcabin",
    "description": "UDropCabin is an unOfficial Dropbox app. It can access Dropbox files and transfer them to other apps",
    "framework": "ubuntu-sdk-15.04-qml",
    "architecture": "armhf",
    "title": "uDropCabin",
    "hooks": {
        "uDropCabin": {
            "apparmor": "uDropCabin.json",
            "content-hub": "uDropCabin-content.json",
            "desktop": "uDropCabin.desktop",
            "account-application": "com.ubuntu.developer.bobo1993324.udropcabin_uDropCabin.application",
            "account-service": "com.ubuntu.developer.bobo1993324.udropcabin_uDropCabin.service"
        },
        "account-plugin": {
            "account-provider": "com.ubuntu.developer.bobo1993324.udropcabin_account-plugin.provider",
            "account-qml-plugin": "account_plugin"
        }
    },
    "version": "0.2.2",
    "maintainer": "Boren Zhang <bobo1993324@gmail.com>"
}
